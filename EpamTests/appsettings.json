{
	"AllowedHosts": "*",
	"Logging": {
		"Console": {
			"LogLevel": {
				"Default": "Information",
				"Microsoft": "Warning",
				"System": "Warning"
			},
			"FormatterOptions": {
				"TimestampFormat": "yyyy-MM-dd hh:mm:ss",
				"UseUtcTimestamp": true
			}
		}
	},
	"ApplicationUrl": "https://www.epam.com/",
	"WebDriverConfiguration": {
		"Browser": "Chrome",
		"IsHeadless": false,
		"IsMaximized": true,
		"PageLoadTimeout": "00:05:00",
		"ImplicitTimeout": "00:00:10",
		"AsynchronousJSTimeout": "00:01:00",
		"LongTimeout": "00:01:00",
		"MediumTimeout": "00:00:30",
		"SmallTimeout": "00:00:15",
		"PollingInterval": "00:00:05"
	}
}
